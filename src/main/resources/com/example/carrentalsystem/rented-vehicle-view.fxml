<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="861.0" prefWidth="1311.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.carrentalsystem.RentedVehicleController">

    <TableView fx:id="customerRentedTable" layoutX="275.0" layoutY="134.0" prefHeight="626.0" prefWidth="948.0">
        <columns>
            <TableColumn fx:id="customerID" prefWidth="100" text="Customer ID" />

            <TableColumn fx:id="customerName" prefWidth="150" text="Name" />
            <TableColumn fx:id="vehicleID" prefWidth="100" text="Vehicle ID" />

            <TableColumn fx:id="brand" prefWidth="150" text="Brand" />
         <TableColumn fx:id="rentedDate" prefWidth="150" text="Rented Date" />
         <TableColumn fx:id="returnDate" prefWidth="150" text="Return Date" />
         <TableColumn fx:id="rentalFee" prefWidth="150" text="Rental Fee" />
<!--         <TableColumn fx:id="colVehiclePic" prefWidth="150" text="Image" />-->
        </columns>
    </TableView>
   <Label layoutX="10.0" layoutY="10.0" style="-fx-font-size: 20px; -fx-text-fill: #2c3e50;" text="Rented Vehicles Dashboard">
      <graphic>
         <ImageView>
            <image>
               <Image url="@icons/dashboard.png" />
            </image>
         </ImageView>
      </graphic>
      <font>
         <Font size="24.0" />
      </font>
   </Label>
   <Button layoutX="687.0" layoutY="779.0" mnemonicParsing="false" onAction="#returnVehicleBtn" prefHeight="37.0" prefWidth="148.0" text="Returned">
      <font>
         <Font name="System Bold" size="14.0" />
      </font>
   </Button>

</AnchorPane>
